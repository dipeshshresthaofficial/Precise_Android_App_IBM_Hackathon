<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coronago">

    <application
        android:allowBackup="true"
        android:icon="@drawable/profile"
        android:label="@string/app_name"
        android:roundIcon="@drawable/profile"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddActivity"></activity>
        <activity android:name=".GamesActivity"
            android:parentActivityName=".games"/>
        <activity android:name=".games"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Predicted"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".AvailableProducts"
            android:parentActivityName=".AvailabilityActivity"/>
        <activity android:name=".AvailabilityActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".CovidTestActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Register2Activity" />
        <activity
            android:name=".CovidInfo"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UpdateStockActivity2"
            android:parentActivityName=".activity_stock2" />
        <activity
            android:name=".activity_stock2"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".LoginActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".EntertainmentActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>